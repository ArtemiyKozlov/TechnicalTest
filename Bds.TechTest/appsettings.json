{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Engines": [
    {
      "Name": "Bing",
      "UrlBase": "https://www.bing.com",
      "SearchPath": "search?q={0}&first={1}&count=10",
      "PageMultiplier": 10,
      "ResultXPath": "//ol[@id='b_results']/li[@class='b_algo']"
    },
    {
      "Name": "Yahoo",
      "UrlBase": "https://search.yahoo.com",
      "SearchPath": "/search?p={0}&pz=10&b={1}",
      "PageMultiplier": 10,
      "ResultXPath": "//ol[contains(@class, 'searchCenterMiddle')]/li"
    }
  ]
}
